#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    scanf("%d",&a);
    int b=a/4;
    if(a>=100 && a<=200)
    {
        
        printf("%d\n%d\n%d\n%d\n",b,b,b,b+a%4);
    }
else{
    printf("INVALID INPUT");
}
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
