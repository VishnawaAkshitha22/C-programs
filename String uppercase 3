#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char ch1[20];
    int i;
    gets(ch1);
    for(i=0;ch1[i]!='\0';i++)
    {
        if(ch1[i]>=97 && ch1[i]<=122)
        {
            ch1[i]=ch1[i]-32;
        }
    }
    printf("%s",ch1);
    
        
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
