#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int a[5],i,n,min;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0,min=a[0];i<n;i++)
    {
        if(min<a[i]){
            min=a[i];
        }
    }
        printf("%d",min);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
